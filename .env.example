# Bot configuration
DEBUG_MODE=false
TOKEN=your_bot_token_here
FEEDBACK_WEBHOOK=https://discord.com/api/webhooks/your_webhook_id/your_webhook_token
MASTER_ENCRYPTION_KEY=your_master_encryption_key_here

# Default Chatbot API Configuration (Bot Owner Settings)
DEFAULT_CHATBOT_API_KEY=your_openai_or_other_api_key_here
DEFAULT_CHATBOT_MODEL=gpt-4o-mini
DEFAULT_CHATBOT_BASE_URL=https://api.openai.com/v1

# Database configuration
POSTGRES_USER=saltbot
POSTGRES_PASSWORD=strong_password_here
POSTGRES_DB=saltbot
## Note: Do not use docker postgresdb in production, use a managed database service instead.like neondb https://console.neon.tech/
POSTGRES_URI=postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@postgres:5432/${POSTGRES_DB} 